<%include partials/_header%>

<div class="container mt-4">
    <div class="row">
        <div class="col-md-5">
            <div class="card">
                <div class="card-body">
                    <form action="/update/<%= data.id%>" method="post">
                        <div class="form-group">
                            <input type="text" name="nombre" id="" value="<%= data.nombre%>" class="form-control">
                                               
                        </div>
                    
                        <div class="form-group">
                            <input type="text" name="direccion" id="" value="<%= data.direccion%>" class="form-control">
                                                   
                        </div>
                    
                        <div class="form-group">
                            <input type="text" name="telefono" id="" value="<%= data.telefono%>" class="form-control">
                                                       
                        </div>
                    
                        <div>
                            <button type="submit" class="btn btn-primary">Edit Customer</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

<%include partials/_footer%>